{
  "private": true,
  "workspaces": [
    "example",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:exma && yarn build:next",
    "build:exma": "yarn --cwd packages/adent-exma build",
    "build:next": "yarn --cwd packages/adent-next build",
    "exma": "yarn --cwd example exma",
    "dev": "yarn --cwd example dev"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}